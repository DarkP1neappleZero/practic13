ip_address = input("Введите Ip адрес: ")

#Разделяем строку на части по точке
parts = ip_address.split('.')

#Проверяем количество частей
if len(parts) != 4:
    print("НЕТ")
else:
    #Флаг для проверки
    valid = True

    #Проверяем каждую часть
    for part in parts:
        #Проверка: часть не должна быть пустой
        if part == "":
            valid = False
            break

        #Проверка: все символы должны быть цифрами
        for char in part:
            if char < '0' or char > '9':
                valid = False
                break
        if not valid:
            break

        #Проверка: число в диапазоне 0-255
        num = int(part)
        if num < 0 or num > 255:
            valid = False
            break

    if valid:
        print("Да")
    else:
        print("Нет")
