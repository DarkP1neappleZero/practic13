numbers = [8, 9, 10, 11]
print(f"Исходный список: {numbers}")
#Заменил второй (по порядку) элемент списка на 17.
numbers[1] = 17
print(f"Заменили второй элемент: {numbers}")
#Добавил числа 4, 5 и 6 в конец списка.
numbers.append(4)
numbers.append(5)
numbers.append(6)
print(f"Добавили 4,5,6 в конец: {numbers}")
#Удалил первый (по порядку) элемент списка.
del numbers[0]
print(f"Удалили первый элемент: {numbers}")
#Удвоил список (добавил к нему все его элементы в том же порядке).
numbers_copy = numbers.copy()
numbers.extend(numbers_copy)
print(f"Удвоили список: {numbers}")
#Вставил число 25 по индексу 3.
numbers.insert(3, 25)
print(f"Вставили 25 по индексу 3: {numbers}")
#Вывел список с помощью функции print().
print(f"Итоговый список: {numbers}")
