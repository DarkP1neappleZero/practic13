numbers_input = input("Введите числа через пробел: ")
parts = numbers_input.split()

#Создаем список чисел
numbers = []
for part in parts:
    numbers.append(int(part))

print(f"Список чисел: {numbers}")

count = 0
#Берем каждое число и сравниваем его
for i in range(len(numbers)):
    for second in range(i + 1, len(numbers)):
        if numbers[i] == numbers[second]:
            count += 1

print(f"Количество пар чисел: {count}")
